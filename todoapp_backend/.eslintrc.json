{
    "env": {
        "node": true
    },
    "extends": ["airbnb-base", "plugin:security/recommended", "prettier"],
    "plugins": ["security", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-console": "error",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "jest/expect-expect": "off",
        "security/detect-object-injection": "off"
    }
}
